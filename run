#!/bin/sh
if [ ! -z "$1" ] ;then
clear
find -iname '*.ko' -exec rm {} /home/anryl/build/$3/system/lib/modules/ \;

COMPILER=/home/anryl/GPL_toolchains/bin/arm-eabi-
